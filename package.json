{
  "name": "bible-memo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild": "npx expo prebuild --clean",
    "web": "expo start --web",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "typecheck": "tsc"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/metro.config.js",
      "!**/jest.setup.js",
      "!**/.expo/**",
      "!**/mocks/**",
      "!**/test-utils/**",
      "!**/builders/**",
      "!**/expo-env.d.ts",
      "!**/app/**"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|@unimodules/.*|unimodules|sentry-expo|@react-navigation/.*)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^7.0.14",
    "expo": "^53.0.10",
    "expo-font": "~13.3.1",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-splash-screen": "~0.30.8",
    "expo-sqlite": "^15.2.12",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "react": "19.0.0",
    "react-dom": "^19.0.0",
    "react-native": "0.79.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "tinybase": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@faker-js/faker": "^8.4.1",
    "@jackfranklin/test-data-bot": "^2.1.0",
    "@tanstack/eslint-plugin-query": "^5.32.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.1",
    "@types/react": "~19.0.10",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.5",
    "msw": "^2.2.14",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
